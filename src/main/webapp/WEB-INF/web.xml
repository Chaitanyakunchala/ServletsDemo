<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://jakarta.ee/xml/ns/jakartaee"
	xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
	id="WebApp_ID" version="6.0">

	<servlet>
		<servlet-name>HalcyonServlet</servlet-name>
		<servlet-class>com.halcyon.HalcyonServlet</servlet-class>

		<init-param>
			<param-name>name</param-name>
			<param-value>ServletConfig : HelcyHalcyon</param-value>
		</init-param>
	</servlet>

	<context-param>
		<param-name>namee</param-name>
		<param-value>ServletContext : HelcyHalcyon</param-value>
	</context-param>

	<servlet-mapping>
		<servlet-name>HalcyonServlet</servlet-name>
		<url-pattern>/Helcyy</url-pattern>
	</servlet-mapping>
</web-app>